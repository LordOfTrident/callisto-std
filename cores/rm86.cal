		"mov [bx], ax"
	end
end

func dup begin
	__rm86_pop_ax
	__rm86_push_ax
	__rm86_push_ax
end

func drop begin asm
	"sub si, 2"
end end

func swap begin
	__rm86_pop_bx
	__rm86_pop_ax
	__rm86_push_bx
	__rm86_pop_ax
end

func + begin
	__rm86_pop_bx
	__rm86_pop_ax
	asm
		"add ax, bx"
	end
	__rm86_push_ax
end

func s+ begin + end

func - begin
	__rm86_pop_bx
	__rm86_pop_ax
	asm
		"sub ax, bx"
	end
	__rm86_push_ax
end

func s- begin - end


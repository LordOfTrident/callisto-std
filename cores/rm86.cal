## Supported features: IO
##
## Cell size = 16 bits
restrict IO
restrict Exit
restrict Time
restrict File

inline __rm86_pop_ax begin asm
	"sub si, 2"
	"mov ax, [si]"
end end

inline __rm86_pop_bx begin asm
	"sub si, 2"
	"mov bx, [si]"
end end

inline __rm86_pop_di begin asm
	"sub si, 2"
	"mov di, [si]"
end end

inline __rm86_pop_dx begin asm
	"sub si, 2"
	"mov cx, [si]"
end end

inline __rm86_pop_cx begin asm
	"sub si, 2"
	"mov cx, [si]"
end end

inline __rm86_push_ax begin asm
	"mov [si], ax"
	"add si, 2"
end end

inline __rm86_push_bx begin asm
	"mov [si], bx"
	"add si, 2"
end end

inline __rm86_push_dx begin asm
	"mov [si], dx"
	"add si, 2"
end end

version IO
	func printch begin
		__rm86_pop_ax
		asm
			"mov ah, 0x0E"
			"int 0x10"
		end
	end
end

func = begin
	__rm86_pop_ax
	__rm86_pop_bx
	asm
		"cmp ax, bx"
		"je .push_1"
		"mov word [si], 0"
		"add si, 2"
		"ret"
		".push_1:"
		"mov word [si], 0xFFFF"
		"add si, 2"
	end
end

func > begin
	__rm86_pop_bx
	__rm86_pop_ax
	asm
		"cmp ax, bx"
		"jg .push_1"
		"mov word [si], 0"
		"add si, 2"
		"ret"
		".push_1:"
		"mov word [si], 0xFFFF"
		"add si, 2"
	end
end


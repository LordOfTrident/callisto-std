func a< begin
	let addr var
	var !
	var @ Array.elements + !
	var @ Array.memberSize + !
	var @ !
end

func a@ begin
	let addr  arr
	let usize offset
	arr !
	offset !

	arr @ Array.elements + offset @ + @
end

func a! begin
	let addr  arr
	let usize offset

	arr !
	offset !

	arr @ Array.elements 2 * + offset @ + !
end

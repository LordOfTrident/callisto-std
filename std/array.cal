func a< begin
	let addr var
	var !
	var @ usize.sizeof 2 * + !
	var @ usize.sizeof + !
	var @ !
end

func a@ begin
	let addr  arr
	let usize offset
	arr !
	offset !

	arr @ usize.sizeof 2 * + offset @ + @
end

func a! begin
	let addr  arr
	let usize offset

	arr !
	offset !

	arr @ usize.sizeof 2 * + offset @ + !
end
